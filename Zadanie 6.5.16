#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define OK 1
#define FAIL 0

typedef struct TWN {
	float data;
	struct TWN *prev;
	struct TWN *next;
}TWN;

typedef struct CTWL {
	TWN *cur;
}CTWL;

void ctwl_cur_step_right(CTWL *list) 
{
	if (list->cur == NULL) 
	{
		return;
	}

	list->cur = list->cur->next;
}
